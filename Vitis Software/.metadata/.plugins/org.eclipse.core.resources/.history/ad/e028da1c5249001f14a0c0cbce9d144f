#include "io_rw.h"
#include "io_map.h"

#define DATA_REG 			0x0

//Handle for the GPI Core
typedef struct{
	uint32_t base_addr;
}GPI_Handle_t;

/*
 * @brief Init the GPI object with the base address
 */
void GPI_Init(GPI_Handle_t* self, uint32_t base_core_addr);

uint32_t GPI_Read(GPI_Handle_t* self);
